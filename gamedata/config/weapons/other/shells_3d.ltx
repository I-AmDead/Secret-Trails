; Базовая секция
[shell_object]:identity_immunities, can_spawner
class				= 3D_SHELL
GroupControlSection	= spawn_group
discovery_dependency =
$spawn			= "weapons\ammo\ammo_gauss" ; option for Level Editor
cform           = skeleton

description		= ui_shell_name
$prefetch 		= 64

cost			= 1000

box_size		= 1

inv_name		= ui_shell_name
inv_name_short	= ui_shell_name
inv_weight		= 0.0

inv_grid_width	= 1
inv_grid_height	= 1
inv_grid_x		= -10
inv_grid_y		= -10

k_dist			= 1
k_disp			= 1
k_hit			= 1.4
k_impulse		= 1
k_pierce		= 15
k_ap			= 15
impair			= 1
buck_shot		= 1
tracer			= off
wm_size         = 0.09

explosive		= off

