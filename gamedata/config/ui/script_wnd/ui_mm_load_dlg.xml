<window>
    <back_video x="0" y="0" width="1024" height="430">
        <texture x="0" y="0" width="1024" height="430"></texture>
    </back_video>

    <back_movie x="0" y="0" width="1024" height="768" stretch="1">
        <texture width="1920" height="1080">ui\menu_video</texture>
    </back_movie>

    <background x="0" y="0" width="1024" height="768">
        <texture x="0" y="0" width="1024" height="768"></texture>
    </background>

    <form x="0" y="0" width="1024" height="768" stretch="1">
        <texture>ui_menu_options_dlg</texture>

        <caption x="490" y="20" width="500" height="25" complex_mode="0">
            <text vert_align="c" font="letterica18" r="184" g="168" b="131">ui_mm_load_game</text>
        </caption>

        <picture x="355" y="65" width="170" height="128" clipper="1" stretch="1">
            <texture>ui\noise\ui_noise</texture>
        </picture>

        <file_info x="20" y="50" width="22" height="22" stretch="1">
            <texture></texture>
        </file_info>

        <file_caption x="535" y="70" width="300" height="40" complex_mode="1">
            <text vert_align="c" font="letterica18" r="238" g="155" b="23"/>
        </file_caption>

        <file_data x="535" y="110" width="512" height="40" complex_mode="1">
            <text vert_align="c" font="letterica18" r="216" g="186" b="140"/>
        </file_data>

        <list_frame x="20" y="50" width="768" height="650">
            <texture></texture>
        </list_frame>

        <list x="20" y="230" width="990" height="520" item_weight="100" item_height="40">
            <font vert_align="c" font="letterica16" r="216" g="186" b="140" />
            <texture>ui_actor_news</texture>
        </list>

        <btn_load x="580" y="200" width="80" height="20" stretch="1">
            <text x="-25" align="r" vert_align="c" font="letterica18">ui_mm_load</text>
            <texture>ui_button_forward</texture>
        </btn_load>

        <btn_delete x="465" y="200" width="80" height="20" stretch="1">
            <text align="c" vert_align="c" font="letterica18">ui_mm_delete</text>
            <texture>ui_button_middle</texture>
        </btn_delete>

        <btn_cancel x="330" y="200" width="80" height="20" stretch="1">
            <text x="25" align="l" vert_align="c" font="letterica18">ui_mm_cancel</text>
            <texture>ui_button_back</texture>
        </btn_cancel>
    </form>
</window>