<window>
    <back_video x="0" y="0" width="1024" height="430">
        <texture x="0" y="0" width="1024" height="430"></texture>
    </back_video>

    <back_movie x="0" y="0" width="1024" height="768" stretch="1">
        <texture width="1920" height="1080">ui\menu_video</texture>
    </back_movie>

    <background x="0" y="0" width="1024" height="768">
        <texture x="0" y="0" width="1024" height="768"></texture>
    </background>

    <form x="0" y="0" width="1024" height="768" stretch="1">
        <texture>ui_menu_options_dlg</texture>

        <caption x="490" y="20" width="500" height="25" complex_mode="0">
            <text font="letterica18" r="184" g="168" b="131">ui_mm_load_game</text>
        </caption>

        <picture x="25" y="55" width="170" height="128" clipper="1" stretch="1">
            <texture>ui\noise\ui_noise</texture>
        </picture>

        <file_info x="20" y="50" width="470" height="138" stretch="1">
            <texture x="975" y="370" width="48" height="22">ui\ui_common</texture>
        </file_info>

        <file_caption x="200" y="60" width="300" height="20" complex_mode="1">
            <text font="letterica18" r="238" g="155" b="23"/>
        </file_caption>

        <file_data x="200" y="80" width="512" height="50" complex_mode="1">
            <text font="letterica18" r="216" g="186" b="140"/>
        </file_data>

        <list_frame x="20" y="50" width="768" height="650">
            <texture></texture>
        </list_frame>

        <list x="20" y="220" width="990" height="530" item_weight="100" item_height="18">
            <font font="letterica16" r="216" g="186" b="140" />
        </list>

        <btn_load x="20" y="190" width="100" height="30">
            <text align="c" font="letterica16" r="227" g="199" b="178">ui_mm_load_game</text>
            <texture>ui_button_ordinary</texture>
        </btn_load>

        <btn_delete x="135" y="190" width="100" height="30">
            <text align="c" font="letterica16" r="227" g="199" b="178">ui_mm_delete</text>
            <texture>ui_button_ordinary</texture>
        </btn_delete>

        <btn_cancel x="250" y="190" width="100" height="30">
            <text align="c" font="letterica16" r="227" g="199" b="178">ui_mm_cancel</text>
            <texture>ui_button_ordinary</texture>
        </btn_cancel>
    </form>
</window>