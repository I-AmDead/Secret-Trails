		<level_map spot="af_cristall_spot"		pointer="quest_pointer"/>
		<mini_map spot="af_cristall_spot_mini"	pointer="quest_pointer"/>
	</spot_af_cristall>
	<af_cristall_spot  x="0" y="0" width="24" height="24" stretch="1" alignment="c" xform_anim="map_spot_new_xform" xform_anim_cyclic="0">
		<texture x="700" y="200" width="50" height="50">ui\ui_icon_equipment</texture>
	</af_cristall_spot>
	<af_cristall_spot_mini  x="0" y="0" width="12" height="12" stretch="1" alignment="c" xform_anim="map_spot_new_xform" xform_anim_cyclic="0">
		<texture x="700" y="200" width="50" height="50">ui\ui_icon_equipment</texture>
		<texture_below r="0" g="255" b="0">ui_mini_sn_spot_below</texture_below>
		<texture_above r="0" g="255" b="0">ui_mini_sn_spot_above</texture_above>
	</af_cristall_spot_mini>

