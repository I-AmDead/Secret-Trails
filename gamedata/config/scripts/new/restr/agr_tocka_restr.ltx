[logic] 
active = {+agro_varayg_four_start -agro_varayg_four_have} sr_idle@4, {+agro_varayg_tri_start -agro_varayg_tri_have} sr_idle@3, {+agro_varayg_two_start -agro_varayg_two_have} sr_idle@2, sr_idle@1

[sr_idle@1] 
on_actor_inside = {+agro_varayg_one_start} sr_idle@2 %=braad_test.seredina_est +agro_varayg_one_have% 

[sr_idle@2] 
on_actor_inside = {+agro_varayg_two_start} sr_idle@3 %=braad_test.seredina_est +agro_varayg_two_have% 

[sr_idle@3] 
on_actor_inside = {+agro_varayg_tri_start} sr_idle@4 %=braad_test.seredina_est +agro_varayg_tri_have% 

[sr_idle@4] 
on_actor_inside = {+agro_varayg_four_start} nil %=braad_test.seredina_est +agro_varayg_four_have%

