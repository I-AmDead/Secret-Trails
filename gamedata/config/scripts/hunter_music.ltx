[logic]
active                                   = ph_sound@on

[ph_sound@on]
tips                                     = switcher_on
nonscript_usable                         = false
on_use                                   = %+hunter_mafon_off =play_snd_from_obj(device\click10b)%
snd                                      = hunter_music
snd_type                                 = music

on_info                                  = {+hunter_mafon_off} ph_sound@off

[ph_sound@off]
tips                                     = switcher_off
nonscript_usable                         = false
on_use                                   = %-hunter_mafon_off =play_snd_from_obj(device\click10b)% ph_sound@on
