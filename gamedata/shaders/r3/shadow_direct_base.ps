#include "common.h"
#include "sload.h"

#ifdef USE_AREF
float4 main(p_shadow_direct I) : SV_Target
#else
float4 main() : SV_Target
#endif
{
#ifdef USE_AREF
    float4 C = s_base.Sample(smp_linear, I.tc0);
    hashed_alpha_test(I.tc0.xy, C.a);
    return C;
#else
    return float4(0.f, 0.f, 0.f, 0.f);
#endif
}
